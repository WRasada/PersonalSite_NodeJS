{{> header}}
  <body id="contactPage">
    {{> navbar}}
    <div class="contactHeader">
      <h2></h2>
    </div>
    <div class="container">
      <div class="row">
        <div class="col-xs-12">
          <div class="contactForm">
            <form action="/contact" method="POST">
              <div class="form-group">
                <label for="email">Email:</label>
                <input type="email" class="form-control" id="email" placeholder="Email" name="email">
              </div>
              <div class="form-group">
                <label for="title">Title:</label>
                <input type="text" class="form-control" id="title" placeholder="Title" name="title">
              </div>
              <div class="form-group">
                <label for="message">Message:</label>
                <textarea name="text" class="form-control" rows="3" placeholder="How can I help you?"></textarea>
              </div>
              <button type="submit" disabled="disabled" class="btn btn-default">Send</button>
              <span id="helpBlock" class="help-block">Disabled temporarily</span>
            </form>
          </div>
        </div>
      </div>

    </div>
  {{> footer}}
  <script type="text/javascript" src="/assets/js/personalSite.js"></script>
  <script type="text/javascript" src="/assets/js/lib/bootstrap.min.js"></script>
  </body>
</html>
